{
    "compilerOptions": {
        "outDir": "dist", // 输出路径
        "module": "ESNext", // 输出类型
        "target": "ES5", // 编译js版本
        "declaration": true, // 生成.d.ts文件
        "jsx": "react",
        "moduleResolution": "node",
        "allowSyntheticDefaultImports": true
    },
    "include": [
        "src"
    ],
    "exclude": [
        "src/**/*.test.tsx",
        "src/**/*.stories.tsx"
    ]
}